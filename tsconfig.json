{
  "compilerOptions": {
    "target": "ESNext",
    "baseUrl": ".",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true
  },
  "include": [
    "TS/**/*",
    "Dashboard/TS/**/*",
    "Dashboard/ckeditor5/**/*",
    ".eslintrc.js"
  ],
  "exclude": ["node_modules", "Dashboard/node_modules"]
}
